@font-face {
    font-family: 'Rockwell';
    src: url('../fonts/rockwell.woff') format('woff'),
       url('../fonts/rockwell.ttf')  format('truetype');
}
@font-face {
    font-family: 'Sketch Rockwell';
    src: url('../fonts/sketch-rockwell.woff') format('woff'),
       url('../fonts/scetch-rockwell.ttf')  format('truetype');
}
* {
    font-family: 'Rockwell', sans-serif;
}
input:focus, button:focus, select:focus, textarea:focus {
    outline: none;
}
a:hover {
	text-decoration: none;
}
.clear {
	clear: both;
	border: none;
	margin: 0;
	padding: 0;
}
.no-display {
    display: none;
}
.no-padding {
    padding: 0 !important;
}
.full-width {
    width: 100%;
}
.navbar {
	background-color: transparent;
    border: none;
    border-radius: 0;
    z-index: 1;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
.sticky.navbar {
	background-color: #feb041;
	position: fixed;
	z-index: 99999;
}
.sticky .navbar-brand img {
	width: 90px;
}
.sticky .navbar-nav {
	padding: 35px 0;
}
.navbar-nav .active a {
    border-radius: 10px;
    margin-bottom: 1px;
    color: #feb041 !important;
    background-color: #821638 !important;
}
.navbar-brand {
	height: auto;
}
.navbar-nav {
	padding: 50px 0;
}
.navbar-nav a {
	color: #fff !important;
	font-size: 20px;
	font-weight: bold;
}
.sticky .navbar-nav a {
	color: #821638  !important;
}
.sticky .navbar-nav .active a {
    border-radius: 10px;
    margin-bottom: 1px;
    color: #feb041 !important;
    background-color: #821638 !important;
}
.navbar-header button {
	margin: 62px 20px;
}
#homeCarousel .carousel-indicators {
	bottom: 100px;
}
.carousel-indicators .active {
	width:12px;
	height:12px;
	margin:0;
	background-color:#821638;
}
.dropdown a:hover, .dropdown:hover a {
	background-color: transparent;
	color: #feb041 !important;
	border-radius: 10px;
	line-height: 15px;
}
.sticky .dropdown a:hover, .sticky .dropdown:hover a {
    background-color: #821638;
    color: #feb041 !important;
}
.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus, .navbar-default .navbar-nav>.open>a:hover {
	background-color: #821638;
	color: #feb041 !important;
	border-radius: 10px;
	line-height: 15px;
}
.dropdown li {
	border-bottom: 2px solid #feb041;
	padding: 10px 0px;
}
.dropdown li:last-child {
	border-bottom: 0px;
}
.navbar-nav li > a:hover, .navbar-nav:hover > a {
    background-color: transparent !important;
    border-radius: 10px;
    margin-bottom: 1px;
    color: #feb041 !important;
}
.sticky .navbar-nav li > a:hover, .sticky .navbar-nav:hover > a {
	background-color: #821638 !important;
}
.dropdown li:hover > a {
    background: transparent;
    font-size: 21px !important;
}
.dropdown:hover ul {
    display: block;
    background-color: transparent;
    box-shadow: none;
    border: 1px solid #feb041;
    border-radius: 10px;
    padding: 0;
}
.sticky .dropdown:hover ul {
	background-color: #821638;
    border: 1px solid #feb041;
}
.dropdown ul a {
	color: #feb041 !important;
    font-size: 20px;
    font-weight: bold;
    line-height: 15px;
}
.slider-panel {
    text-align: center;
    background: transparent;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -322px;
    margin-top: -100px;
    z-index: 2;
}
#homeCarousel .slider-panel {
    width: 644px;
}
#homeCarousel .slider-panel > div:first-child {
	font-size: 36px;
	color: #fff;
	font-family: 'Open Sans', sans-serif;
}
#homeCarousel .slider-panel > div:first-child span {
	font-size: 38px;
	font-family: 'Sketch Rockwell', sans-serif;
	color: #feb041;
}
#homeCarousel .slider-panel > div:last-child {
	padding: 20px 0;
}
#homeCarousel .slider-panel button {
    height: 42px;
    width: 136px;
    box-shadow: none;
    border: 1px solid #feb041;
    border-radius: 15px;
    background-color: transparent;
    color: #feb041;
    font-weight: bold;
    font-size: 18px;
    margin: 7px;
}
#homeCarousel .slider-panel button:last-child {
	background-color: #821638;
    color: #feb041;
    border: 1px solid #821638
}
.down-arrow {
	position: absolute;
    bottom: 34px;
    width: 28px;
    height: 28px;
    background: transparent;
    border: 2px solid #fff;
    border-radius: 50%;
    left: 50%;
    margin-left: -14px;
    z-index: 2;
    cursor: pointer;
}
.down-arrow img {
	padding-left: 4px;
    padding-top: 5px;
}
.white-bg {
	padding: 30px 0;
}
.title {
	font-size: 30px;
	font-family: 'Open Sans', sans-serif;
	color: #102b4c;
	font-weight: bold;
	text-align: center;
	padding: 15px;
	line-height: 20px;
    text-transform: uppercase;
}
.sub-title {
	text-align: center;
	line-height: 15px;
}
.sub-title a {
	font-size: 18px;
	color: #c0b48b;
	font-weight: bold;
}
.mission, .vision {
	text-align: center;
}
.mission span, .vision span {
	font-size: 36px;
	font-weight: bold;
	color: #821638;
	margin-left: -75px;
}
.mission img, .vision img {
}
.vision span {
	margin-left: -60px;
}
.mission div, .vision div {
	font-size: 15px;
	color: #821638;
	text-align: center;
	font-weight: 600;
    font-family: 'Open Sans', sans-serif;
    padding: 30px 0;
}
.banner-container {
	height: 37vw;
}
.counter-container {
    height: 23vw;
}
.banner {
	position: absolute;
	left: 0;
}
.banner img {
	width: 100%;
}
.partners .select-container {
	text-align: center;
	margin: 35px 0;
}
.partners .bselect {
	box-shadow: none;
    border: 1px solid #821638;
    border-radius: 10px;
}
.partners .bselect .caret {
	display: none;
}
.partners .bselect-label {
	background: transparent;
    text-shadow: none;
    border: none;
    height: 58px;
    font-size: 18px;
    font-weight: bold;
    color: #821638;
    min-width: 140px !important;
    padding: 20px 0px;
    margin: 0 20px;
}
.partners .bselect .bselect-caret {
	background-image: url("../images/select-arrow.png");
    background-position: 15px;
    background-repeat: no-repeat;
    height: 58px;
    background-color: transparent;
    border: none;
    border-left: 1px solid #821638;
    padding: 0 10px;
    color: #821638;
    width: 30px;
}
.partners .bselect .bselect-option a {
	background: #fff;
    color: #821638;
    font-size: 18px;
}
.partners .bselect .bselect-dropdown {
	box-shadow: none;
    background-color: #ffffff;
    width: 100%;
    border-radius: 10px;
    border: 1px solid #821638;
}
.bselect-option {
    cursor: pointer;
}
.partners .bselect .bselect-option:hover a , .partners .bselect .bselect-option.active a {
    font-size: 20px;
    text-shadow: none;
}
.partners .bselect.open .bselect-caret {
	box-shadow: none;
	background-image: url("../images/select-arrow.png");
}
.partners .flexslider .slides li {
	padding: 10px;
    border: 1px solid #821638;
    border-radius: 10px;
    cursor: pointer;
}
.partners .flexslider .overlay {
	display: none;
	position: absolute;
    background-color: #821638;
    color: #fff;
    top: 0;
    margin-left: -10px;
    width: 185px;
    height: 185px;
    border-radius: 10px;
    text-align: center;
    padding: 70px 0;
}
.partners .flexslider .overlay div:first-child {
	font-family: 'Sketch Rockwell', sans-serif;
	font-size: 26px;
	line-height: 20px;
}
.partners .flexslider .overlay div:last-child {
	font-size: 12px;
	font-weight: bold;
	color: #feb041;
}
.flex-direction-nav a {
	height: 50px;
}
.counter {
	background-image: url("../images/counter-bg.jpg");
	position: absolute;
    width: 100%;
    left: 0;
    margin: 0;
    padding: 95px 0;
    background-repeat: no-repeat;
}
.counter > div {
	text-align: center;
}
.counter > div > div, .counter > div > div span {
	font-family: 'Sketch Rockwell', sans-serif;
	font-size: 55px;
	color: #fff;
}
.counter > div > div:last-child {
	font-size: 32px;
}
.carousel-inner {
    text-align: center;
}
#testCarousel {
    margin-top: 100px;
}
#testCarousel .carousel-control {
    color: #821638;
    text-shadow: none;
    background-image: none !important;
}
#testCarousel .carousel-control:focus, .carousel-control:hover {
    color: #821638;
    text-decoration: none;
    filter: alpha(opacity=90);
    outline: 0;
    opacity: .9;
}
#testCarousel .item img {
    border: 6px solid #821638;
    border-radius: 50%;
    margin-bottom: 30px;
}
#testCarousel .item .text {
    font-weight: 600;
    font-family: 'Open Sans', sans-serif;
    max-width: 900px;
    margin: 0 auto;
    color: #821638;
    font-size: 20px;
    letter-spacing: -1px;
}
#testCarousel .item .author {
    padding: 35px;
}
#testCarousel .item .author span {
    color: #feb041;
    padding: 5px;
    font-size: 23px;
}
#testCarousel button {
    height: 58px;
    padding: 0 55px;
    box-shadow: none;
    border: 1px solid #821638;
    border-radius: 15px;
    background-color: transparent;
    color: #821638;
    font-weight: bold;
    font-size: 22px;
    margin: 7px;
}
#endCarousel .slider-panel {
    left: 0;
    margin-left: 0;
    padding: 0 150px;
}
#endCarousel .logo {
    max-width: 250px;
}
#endCarousel .text div:first-child {
    color: #fff;
    font-size: 36px;
    font-family: 'Open Sans', sans-serif;
    font-weight: bold;
    padding: 30px 0;
}
#endCarousel .text div:last-child {
    color: #fff;
    font-size: 18px;
    font-family: 'Open Sans', sans-serif;
    font-weight: 600;
}
.event-container {
    background-color: #ececec;
    position: absolute;
    left: 0;
    padding: 40px 0;
}
.event {
    padding: 20px;
}
.event .inner-event {
    border: 1px solid #821638;
    border-radius: 5px;
    margin: 20px;
    background-color: #fff;
}
.event .pic {
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    width: 100%;
}
.event .date img {
    max-width: 90px;
}
.event .location img {
    max-width: 60px;
    padding-top: 20px;
    margin-right: 15px;
}
.event .date div:first-child, .event .location div:first-child {
    font-size: 18px;
    font-weight: bold;
    color: #feb041;
    line-height: 70px;
}
.event .text {
    font-family: 'Open Sans', sans-serif;
    font-weight: bold;
    font-size: 20px;
    color: #821638;
}
.event .content {
    font-size: 20px;
    color: #821638;
    font-family: 'Open Sans', sans-serif;
    line-height: 20px;
}
.submit-btn {
    text-align: center;
    margin: 20px 0;
}
.submit-btn button, .more-events button, .job-item button, .major-item button {
    height: 58px;
    padding: 0 55px;
    box-shadow: none;
    border: 1px solid #821638;
    border-radius: 15px;
    background-color: transparent;
    color: #821638;
    font-weight: bold;
    font-size: 22px;
    margin: 7px;
}
.submit-btn button:hover, .more-events button:hover, #testCarousel button:hover, .job-item button:hover, .major-item button:hover {
    background-color: #821638;
    color: #fff;
}
.more-events {
    text-align: center;
}
.jobs-banner {
    margin-top: 120px;
    position: absolute;
}
.jobs-banner .title {
    font-size: 36px;
    font-family: 'Open Sans', sans-serif;
    font-weight: bold;
    color: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -25px;
    margin-left: -153px;
}
.jobs-banner .title span {
    font-family: 'Sketch Rockwell', sans-serif;
    color: #feb041;
}
.jobs, .contact-info {
    padding-top: 315px;
}
.jobs .job-item, .jobs .major-item {
    padding-top: 40px;
    padding-bottom: 60px;
}
.jobs .job-item img, .jobs .major-item img {
    max-width: 150px;
}
.jobs .job-item .text, .jobs .major-item .text {
    font-size: 18px;
    font-family: 'Open Sans', sans-serif;
    color: #821638;
    padding: 0 35px;
    line-height: 20px;
}
.jobs .job-item, .jobs .major-item {    
    text-align: center;
}
.jobs .job-item hr {
    border-top: 5px solid #821638;
    width: 230px;
}
.jobs .major-item hr {
    border-top: 5px solid #feb041;
    width: 230px;
}
.jobs .job-item .job-title, .jobs .major-item .job-title {
    font-size: 32px;
    font-weight: bold;
    padding: 20px;
}
.jobs .job-item .job-title {
    color: #feb041;
}
.jobs .major-item .job-title {
    color: #821638;
}
.contact-info .info-item {
    text-align: center;
}
.message {
    background: #ececec;
}
.message form {
    padding: 40px 0;
}
.message input::-webkit-input-placeholder, .message textarea::-webkit-input-placeholder {
   color: #feb041;
}
.message input:-moz-placeholder, .message textarea:-moz-placeholder { /* Firefox 18- */
   color: #feb041;  
}
.message input::-moz-placeholder, .message textarea::-moz-placeholder {  /* Firefox 19+ */
   color: #feb041;  
}
.message input:-ms-input-placeholder, .message textarea:-ms-input-placeholder {  
   color: #feb041;  
}
.message form input {
    background-color: transparent;
    border: 1px solid #821638;
    border-radius: 10px;
    box-shadow: none;
    color: #feb041;
    height: 38px;
    padding: 10px;
}
.message textarea {
    border: 1px solid #821638;
    border-radius: 10px;
    box-shadow: none;
    color: #feb041;
    height: 200px;
    padding: 10px;   
    background-color: transparent;
}
.message .submit-btn {
        margin: 0;
    font-size: 22px;
    height: 60px;
    font-weight: bold;
    color: #821638;
}

@media (max-width: 767px) {
    .counter {
        padding: 50px 0;
    }
    .counter > div > div, .counter > div > div span {
        font-size: 30px;
    }
    .counter > div > div:last-child {
        font-size: 20px;
    }
}
@media (min-width: 768px) {
.navbar-right .dropdown-menu {
    right: auto;
}